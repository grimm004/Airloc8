<Window x:Class="LocationInterface.Windows.TimeSetterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LocationInterface.Windows"
        mc:Ignorable="d"
        Title="Target Time Setter" Height="60" Width="500" Loaded="OnLoaded" ResizeMode="NoResize">
    <Grid>
        <Slider x:Name="TimeSlider" Grid.Column="4" IsEnabled="False" Minimum="0" Maximum="24" Margin="1,1,1,1" HorizontalAlignment="Stretch" VerticalAlignment="Center" ValueChanged="SliderValueChanged"/>
    </Grid>
</Window>
